{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/components/ui/*": [
        "./src/components/ui/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/blocks/*": [
        "./src/blocks/*"
      ],
      "@/onu/*": [
        "./src/registry/onu/*"
      ],
      "@/demo/*": [
        "./src/app/demo/[name]/*"
      ],
      "@/registry": [
        "./registry.json"
      ],
      "@/foundation/*": [
        "./src/registry/onu/foundation/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "ES2017",
    
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
